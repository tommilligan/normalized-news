{
  "name": "normalized-news",
  "version": "0.0.1",
  "description": "Read the news with anonymized people, locations and organizations.",
  "author": "Tom Milligan <code@tommilligan.net>",
  "private": false,
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "babel src -d lib",
    "test": "mocha --compilers js:babel-core/register -R spec",
    "coverage": "cross-env NODE_ENV=test nyc mocha"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tommilligan/normalized-news.git"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "cheerio": "^1.0.0-rc.2",
    "dotenv-safe": "^4.0.4",
    "gender-neutral": "git+https://github.com/tommilligan/gender-neutral#develop",
    "ner": "^0.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.1",
    "eslint-plugin-mocha": "^4.11.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.4.2",
    "mocha-eslint": "^4.0.0",
    "nyc": "^11.0.3"
  }
}
